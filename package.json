{
  "name": "hosted-hubot",
  "version": "2.7.1",
  "private": true,
  "author": "GitHub Inc.",
  "keywords": [
    "github",
    "hubot",
    "campfire",
    "bot"
  ],
  "config": {
    "blanket": {
      "loader": "./node-loaders/coffee-script"
    }
  },
  "scripts": {
    "start": "./bin/hubot",
    "test": "mocha --compilers coffee:coffee-script/register --ui bdd --timeout 5000 --recursive"
  },
  "description": "A simple helpful robot for your Company",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/github/hubot/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/github/hubot.git"
  },
  "dependencies": {
    "hubot": ">= 2.6.0 < 3.0.0",
    "hubot-scripts": ">= 2.5.0 < 3.0.0",
    "moment": "^2.6.0"
  },
  "engines": {
    "node": ">= 0.8.x",
    "npm": ">= 1.1.x"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^1.9.1",
    "coffee-script": "^1.7.1",
    "mocha": "^1.18.2",
    "mocha-lcov-reporter": "0.0.1"
  }
